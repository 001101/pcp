#!/bin/bash
# PCP QA Test No. 1202
# Replay dmstats instance archive using pmrep
#
# Copyright (c) 2017 Fumiya Shigemitsu.  All Rights Reserved.
#

seq=`basename $0`
echo "QA output created by $seq"

# get standard environment, filters and checks
. ./common.product
. ./common.filter
. ./common.check

status=1 # failure is the default!
trap "_cleanup $iam" 0 1 2 3 15

pmrep -z -a archives/dm-stats dm.read dm.reads_merged dm.read_bytes dm.reads_time \
	dm.write dm.writes_merged dm.write_bytes dm.writes_time \
	dm.in_progress dm.io_ticks dm.queue_ticks dm.queue_ticks dm.read_ticks dm.write_ticks

status=0
exit
