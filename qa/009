#! /bin/sh
# kmchart QA Test No. 009
# Copyright (c) 2008 Aconex.  All Rights Reserved.
# Copyright (c) 1995-2002 Silicon Graphics, Inc.  All Rights Reserved.
#
# Exercise some kmdumptext problems
#
seq=`basename $0`
echo "QA output created by $seq"

# get standard filters
. ./common.filter

tmp=/tmp/$$
status=1	# failure is the default!
trap "rm -f $tmp.*; exit \$status" 0 1 2 3 15

# real QA test starts here
inst=`pminfo -F proc.psinfo.pid \
      | sed -n -e '/inst \[1 /{
s/^[^"]*"//
s/ .*//
p
}'`

if [ -z "$inst" ]
then
    echo "Arrgh, cannot find proc.psinfo.pid for init!"
    pminfo -F proc.psinfo.pid
fi

echo "proc.psinfo.pid[$inst] 0" >$tmp.config
echo "proc.psinfo.pid[1]" >> $tmp.config

kmdumptext -g -s 1 -f '' -t 1 -c $tmp.config >$tmp.out 2>&1

echo
echo "expect two \"1\"s ..."
sort -u $tmp.out

# success, all done
status=0
exit
