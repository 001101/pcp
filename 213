owner=chatz

seq=`basename $0`

if [ -x src-oss/pmc_desc ]
then
    :
else
    echo "Can't execute src-oss/pmc_desc" >$seq.notrun
    echo "$seq: [not run] `cat $seq.notrun`"
    exit 0
fi

echo "QA output created by $seq"

# get standard filters
. ./common.filter
. ./common.check

tmp=/tmp/$$
sudo=`pwd`/sudo
status=1	# failure is the default!
trap "rm -f $tmp.*; exit \$status" 0 1 2 3 15

# real QA test starts here

cd src-oss
./pmc_desc

# success, all done
status=0
exit
