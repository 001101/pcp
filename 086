#! /bin/sh
# PCP QA Test No. 086
# $Revision: 2.8 $
# timezone exercises
#
# Copyright (c) 1995-2002 Silicon Graphics, Inc.  All Rights Reserved.
#

seq=`basename $0`
echo "QA output created by $seq"
# get standard filters
. ./common.product
. ./common.filter

tmp=/tmp/$$
trap "rm -f $tmp.*; exit" 0 1 2 3 15

# real QA test starts here

me=`hostname`

other1=bogus
other2=bogus

eval `./getpmcdhosts -L -n 2 \
      | $PCP_AWK_PROG '{ if (NF == 2) printf("other1=%s\nother2=%s\n",$1,$2); }'`

if [ "X$other1" = Xbogus -o "X$other2" = Xbogus ]
then
    echo "Unable to find two other hosts" >$seq.notrun
    exit 1
fi

#
# only interested in failure or output on stderr
src-oss/whichtimezone $me $other1 $other2 | sed -e '/Trying/d' -e '/^zone/d' -e '/^$/d'
exit $?
