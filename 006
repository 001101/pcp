#! /bin/sh
# PCP QA Test No. 006
# $Revision: 2.11 $
# Check error codes ... 
#
# Copyright (c) 1995-2002 Silicon Graphics, Inc.  All Rights Reserved.
#

seq=`basename $0`
echo "QA output created by $seq"

# get standard filters
. ./common.product
. ./common.filter

tmp=/tmp/$$
trap "rm -f $tmp.*; exit 0" 0 1 2 3 15

PM_ERR_BASE=12345
PM_ERR_COUNT=100

sts=$PM_ERR_BASE
top=`expr $PM_ERR_BASE + $PM_ERR_COUNT`
while [ $sts -lt $top ]
do
    pmerr -$sts
    sts=`expr $sts + 1`
done \
| sed -e 's/0x........ //' \
      -e 's/Code: //' \
      -e 's/Text: //'

