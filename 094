#! /bin/sh
# PCP QA Test No. 094
# $Revision: 2.9 $
# interpolate unsigned long for PM_TYPE_INSTANT was busted when the
# value decreased
#
# Copyright (c) 1995-2002 Silicon Graphics, Inc.  All Rights Reserved.
#

seq=`basename $0`
echo "QA output created by $seq"

# get standard filters
. ./common.product
. ./common.filter

tmp=/tmp/$$
trap "rm -f $tmp.*; exit" 0 1 2 3 15

# real QA test starts here

# note this is a V1 archive, hence the need for the -n src-oss/root_irix
# in the tests below.
#
pmkstat -s 40 -n src-oss/root_irix \
	-a src-oss/babylon.pmview \
| $PCP_AWK_PROG '
/^#/	{ next }
/runq/	{ next }
/mem/	{ print "memory free ... from pmkstat"; next }
$3 > 0	{ print "OK"; next }
	{ print "botched ... " $3 ; print }'

