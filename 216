#!/bin/sh
# PCP QA Test No. 216 (formerly 1173, 444)
# $Revision: 2.20 $
#
# Test functionality of PMC_Context
#
# Copyright (c) 1995-2002,2005 Silicon Graphics, Inc.  All Rights Reserved.
#
# creator
owner=chatz

seq=`basename $0`
. ./localconfig
if [ ! -f src-oss/pmc_context ]
then
    echo "Not run: no src-oss/pmc_context executable" >$seq.notrun
    echo "$seq: [not run] `cat $seq.notrun`"
    exit 0
fi

echo "QA output created by $seq"

# get standard filters
. ./common.filter
. ./common.check

tmp=/tmp/$$
sudo=`pwd`/sudo
status=1	# failure is the default!
trap "rm -f $tmp.*; exit \$status" 0 1 2 3 15

# real QA test starts here

# timezones and EHOSTUNREACH (148 for IRIX, 113 for Linux)
_filter()
{
    sed \
	-e "s#EST-11EST-10,89/2:00,299/2:00#TZ#g" \
	-e 's/-113 has 0/-148 has 0/' \
	-e 's/sts = -113/sts = -148/'
}

cd src-oss
./pmc_context -DPMC 2>&1 | _filter

# success, all done
status=0
exit
