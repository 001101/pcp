#! /bin/sh
# PCP QA Test No. 646
# minimal testing for libpcp_mmv functionality
#
# Copyright (c) 1995-2002 Silicon Graphics, Inc.  All Rights Reserved.
#
# creator
owner=makc

seq=`basename $0`

echo "QA output created by $seq"

# get standard environment, filters and checks
. ./common.product
. ./common.filter
. ./common.check

tmp=/tmp/$$
here=`pwd`
sudo=$here/sudo
status=0	# success is the default!
$sudo rm -rf $tmp.*
trap "rm -f $tmp.*; $sudo rm -f /var/tmp/mmv/test$$; exit \$status" 0 1 2 3 15

$sudo src-oss/mmv_genstats test$$
$PCP_PMDAS_DIR/mmv/mmvdump $PCP_TMP_DIR/mmv/test$$ | \
    sed \
	-e 's/interval = [0-9][0-9]*/interval = TIME/' \
	-e 's/[A-Z][a-z][a-z] [A-Z][a-z][a-z]  *[0-9][0-9]* [0-9][0-9]:[0-9][0-9]:[0-9][0-9] [0-9]*/TIMESTAMP/'

status=0
exit
