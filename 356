# get standard filters
. ./common.product
. ./common.filter
. ./common.check

tmp=/tmp/$$
sudo=`pwd`/sudo
status=1	# failure is the default!
trap "rm -f /tmp/news.* $tmp.*; exit \$status" 0 1 2 3 15

# real QA test starts here
cd /tmp
cat <<End-of-File | $PCP_PMDAS_DIR/news/pmdanews
PROFILE 0 0 0
FETCH 0 1 0 0
. 117440713
DESC_REQ 117440713
FETCH 0 1 0 0
. 117440813
FETCH 0 1 0 0
. 117440814
FETCH 0 1 0 0
. 117440613
FETCH 0 1 0 0
. 117440623
FETCH 0 1 0 0
. 117440624
FETCH 0 1 0 0
. 117440625
FETCH 0 1 0 0
. 117440626
FETCH 0 8 0 0
. 117440713
. 117440813
. 117440814
. 117440613
. 117440623
. 117440624
. 117440625
. 117440626
DESC_REQ 117440713
DESC_REQ 117440813
INSTANCE_REQ 117440513 ? ?
DESC_REQ 117440814
DESC_REQ 117440613
DESC_REQ 117440623
DESC_REQ 117440624
DESC_REQ 117440625
DESC_REQ 117440626
DESC_REQ 117440626
End-of-File

# success, all done
status=0
