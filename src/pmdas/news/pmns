news {
    articles
    readers
}

news.readers {
	nnrpd		NEWSAGENT:0:101
	rn		NEWSAGENT:0:111
	trn		NEWSAGENT:0:112
	xrn		NEWSAGENT:0:113
	vn		NEWSAGENT:0:114
}

news.articles {
	total		NEWSAGENT:0:201
	count		NEWSAGENT:0:301
	last		NEWSAGENT:0:302
}
