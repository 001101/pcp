systemd {
    numclients		SYSTEMD:0:0
    maxmem		SYSTEMD:0:1
    journal
}

systemd.journal {
    /* systemd.journal-fields */
    cursor              SYSTEMD:1:0
    string              SYSTEMD:1:1 /* pure ascii */
    blob                SYSTEMD:1:2 /* not pure ascii */
    /* the records themselves */
    records             SYSTEMD:2:0
    records_raw         SYSTEMD:2:1
}
