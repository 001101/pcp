    chunk_size			MEMCACHE:1:0
    chunks_per_page		MEMCACHE:1:1
    total_pages			MEMCACHE:1:2
    total_chunks		MEMCACHE:1:3
    used_chunks			MEMCACHE:1:4
    free_chunks			MEMCACHE:1:5
    free_chunks_end		MEMCACHE:1:6
}

