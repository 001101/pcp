dmthin {
    pool
    vol
}

dmthin.pool {
    metadata_used		DMTHIN:0:0
    metadata_total		DMTHIN:0:1
    data_used			DMTHIN:0:2
    data_total			DMTHIN:0:3
    held_root			DMTHIN:0:4
    discard_passdown		DMTHIN:0:5
    read_mode			DMTHIN:0:6
    no_space_mode		DMTHIN:0:7
}

dmthin.vol {
    num_mapped_sectors		DMTHIN:1:0
    highest_mapped_sector	DMTHIN:1:1
}
