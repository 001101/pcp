root {
	kernel
	mem
	swap
	disk
}

kernel {
	all
	percpu
}

kernel.all {
	load		60:2:0	/* linux.loadavg */
	pswitch		60:0:13	/* linux.stat.ctxt */
	cpu
}

kernel.all.cpu {
	user		60:0:20 /* linux.stat.all.user
	sys		60:0:22 /* linux.stat.all.sys
	idle		60:0:23 /* linux.stat.all.idle
}

kernel.percpu {
	cpu
}

kernel.percpu.cpu {
	user		60:0:0 /* linux.stat.user */
	sys		60:0:2 /* linux.stat.sys */
	idle		60:0:3 /* linux.stat.idle */
}

mem {
	physmem		60:1:0 /* linux.meminfo.total */
	freemem		60:1:2 /* linux.meminfo.free */
	bufmem		60:1:4 /* linux.meminfo.buffers */
}

swap {
	length		60:1:6 /* linux.meminfo.swap_total */
	used	 	60:1:7 /* linux.meminfo.swap_used */
	free	 	60:1:8 /* linux.meminfo.swap_free */
	pagesin		60:0:8 /* linux.stat.page_in */
	pagesout	60:0:9 /* linux.stat.page_out */
	in		60:0:10 /* linux.stat.swap_in */
	out		60:0:11 /* linux.stat.swap_out */
}

disk {
	dev
	all
}

disk.dev {
        read		60:0:4 /* linux.stat.rio */
        write		60:0:5 /* linux.stat.wio */
        blkread		60:0:6 /* linux.stat.rblk */
        blkwrite	60:0:7 /* linux.stat.wblk */
}

disk.all {
	read		60:0:24 /* linux.stat.all.rio */
	write		60:0:25 /* linux.stat.all.wio */
	blkread		60:0:26 /* linux.stat.all.rblk */
	blkwrite	60:0:27 /* linux.stat.all.wblk */
}
