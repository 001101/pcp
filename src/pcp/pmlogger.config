#
# pmlogger(1) configuration file suitable for creating an archive to be
# used with pcp(1)
#

log mandatory on once {
    hinv.ncpu
    hinv.ndisk
    hinv.nnode
    hinv.nrouter
    hinv.nxbow
    hinv.ncell
    hinv.physmem
    hinv.cputype
    pmda.uname
    pmcd.numagents
    pmcd.numclients
    pmcd.simabi
    pmcd.version
    pmcd.build
    pmcd.license
    pmcd.timezone
    pmcd.agent.status
    pmcd.pmlogger
    pmcd.pmie
}
