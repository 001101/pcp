#pmlogconf-setup 2.0
ident	Linux swap, cache and networking configuration
probe	mem.slabinfo.objects.size exists ? include : exclude
delta	once
	swap.length
	mem.slabinfo.objects.size [ ext3_inode_cache dentry_cache buffer_head ]
	network.interface.mtu
	network.interface.speed
	network.interface.duplex
	network.interface.inet_addr

