	network.tcp.timeoutdrop
	network.udp.ipackets
	network.udp.opackets
	rpc.client.badxid
	rpc.client.calls
	rpc.client.timeout
	swap.free
	swap.length
	swap.pagesout
	xfs.log_blocks
	xfs.log_noiclogs
	xfs.log_writes
	xfs.read_bytes
	xfs.write_bytes
}
