#pmlogger Version 1
#
#
#
# basic pmlogger config file sufficient for ...
#
#	pmchart Overview
#	osvis
#	dkvis
#	mpvis
#	pmkstat
#	pmlogconf default selections
#	pmieconf rules
#

log mandatory on once { hinv }

log advisory on default {
	hw.hub.ii.cb_errors
	hw.hub.ni.cb_errors
	hw.router.perport.cb_errors
}

log advisory on default {
	disk.all.active
	disk.all.avg_disk.active
	disk.all.bytes
	disk.all.read
	disk.all.read_bytes
	disk.all.total
	disk.all.write
	disk.all.write_bytes
	disk.ctl.avg_disk.active
	disk.dev.active
	disk.dev.bytes
	disk.dev.read
	disk.dev.read_bytes
	disk.dev.total
	disk.dev.write
	disk.dev.write_bytes
	filesys.capacity
	filesys.used
	kernel.all.cpu.idle
	kernel.all.cpu.intr
	kernel.all.cpu.sxbrk
	kernel.all.cpu.sys
	kernel.all.cpu.user
	kernel.all.cpu.wait.total
	kernel.all.intr.non_vme
	kernel.all.io.bread
	kernel.all.io.iget
	kernel.all.io.lread
	kernel.all.io.namei
	kernel.all.kpreempt
	kernel.all.kswitch
	kernel.all.load
	kernel.all.pswitch
	kernel.all.runocc
	kernel.all.runque
	kernel.all.swap.swpocc
	kernel.all.swap.swpque
	kernel.all.syscall
	kernel.all.sysexec
	kernel.all.sysfork
	kernel.all.sysioctl
	kernel.all.sysread
	kernel.all.syswrite
	kernel.all.users
	kernel.percpu.cpu.intr
	kernel.percpu.cpu.sys
	kernel.percpu.cpu.user
	kernel.percpu.cpu.idle
	kernel.percpu.cpu.wait.total
	kernel.percpu.io.iget
	kernel.percpu.io.namei
	kernel.percpu.syscall
	nfs.client
	nfs.server
	nfs3.client
	nfs3.server
	mem.freemem
	mem.util
	mem.util.free
	mem.util.fs_clean
	mem.util.fs_ctl
	mem.util.fs_dirty
	mem.util.kernel
	mem.util.user
	network.interface.baudrate
	network.interface.collisions
	network.interface.in.bytes
	network.interface.in.errors
	network.interface.in.packets
	network.interface.out.bytes
	network.interface.out.errors
	network.interface.out.packets
	network.interface.total.bytes
	network.interface.total.errors
	network.interface.total.packets
