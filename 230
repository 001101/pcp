#! /bin/sh
# PCP QA Test No. 230 (formerly 1177, 458)
# $Revision: 1.29 $
#
# check PMC handles host to archive mapping with multiple groups
#
# Copyright (c) 1995-2002,2005 Silicon Graphics, Inc.  All Rights Reserved.
#
# creator
owner=chatz

seq=`basename $0`
echo "QA output created by $seq"

. ./localconfig
if [ ! -f src-oss/pmc_hosts ]
then
    echo "Not run: no src-oss/pmc_hosts executable" >$seq.notrun
    echo "$seq: [not run] `cat $seq.notrun`"
    exit 0
fi

# get standard filters
. ./common.filter
. ./common.check

tmp=/tmp/$$
sudo=`pwd`/sudo
status=1	# failure is the default!
trap "rm -f $tmp.*; exit \$status" 0 1 2 3 15

_filter()
{
    sed -e 's/: Line [0-9][0-9]*/: Line <N>/'
}

# real QA test starts here

cd src-oss
./pmc_hosts -DPMC 2>&1 | _filter

# success, all done
status=0
exit
