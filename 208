# creator
owner=chatz

seq=`basename $0`

# get standard filters
. ./common.filter
. ./common.check

_check_purify src-oss/pmc_vector

echo "QA output created by $seq"

tmp=/tmp/$$
sudo=`pwd`/sudo
status=1	# failure is the default!
trap "cd /; rm -rf $tmp $tmp.*; exit \$status" 0 1 2 3 15

# real QA test starts here

_setup_purify src-oss/pmc_vector

_run_purify

# success, all done
status=0
exit
