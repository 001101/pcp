#!/bin/sh
# PCP QA Test No. 190 (formerly 1003, and before that 383)
# $Revision: 2.15 $
# test bug in pmConvScale(3)
#
# Copyright (c) 1995-2002 Silicon Graphics, Inc.  All Rights Reserved.

# creator
owner=markgw

seq=`basename $0`
echo "QA output created by $seq"

# get standard filters
. ./common.product
. ./common.filter
. ./common.check

#  Requires common.product:
if [ "$PCP_PLATFORM" = linux ]
then
    echo "[test configuration for Linux]"
    rm -f $seq.out ; ln $seq-linux.out $seq.out
else
    echo "[test configuration for IRIX]"
    rm -f $seq.out ; ln $seq-irix.out $seq.out
fi

tmp=/tmp/$$
status=0	# success is the default!
trap "rm -f $tmp.*; exit \$status" 0 1 2 3 15

# we really on can rely on 6 digits of precision ..
#
_filter()
{
    sed \
	-e 's/123456...........\.[0-9]/123456???????????.?/g' \
	-e 's/123456..........\.[0-9]/123456??????????.?/g' \
	-e 's/123456.........\.[0-9]/123456?????????.?/g' \
	-e 's/123456........\.[0-9]/123456????????.?/g' \
	-e 's/123456.......\.[0-9]/123456???????.?/g' \
	-e 's/123456......\.[0-9]/123456??????.?/g' \
	-e 's/123456.....\.[0-9]/123456?????.?/g' \
	-e 's/123456....\.[0-9]/123456????.?/g' \
	-e 's/123456...\.[0-9]/123456???.?/g' \
	-e 's/123456..\.[0-9]/123456??.?/g' \
	-e 's/123456.\.[0-9]/123456?.?/g'\
	-e 's/123456\.[0-9]/123456.?/g'
}

# real QA test starts here
echo "=== the dimension is NONE case ==="
src-oss/pmconvscale | _filter
status=`expr $status + $?`

echo
echo "=== the dimension is COUNT case ==="
src-oss/pmconvscale c | _filter
status=`expr $status + $?`

echo
echo "=== the dimension is SPACE case ==="
src-oss/pmconvscale s | _filter
status=`expr $status + $?`

echo
echo "=== the dimension is TIME case ==="
src-oss/pmconvscale t | _filter
status=`expr $status + $?`

echo
echo "=== the dimension is SPACE/sec case ==="
src-oss/pmconvscale r | _filter
status=`expr $status + $?`

exit
