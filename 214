#! /bin/sh
# PCP QA Test No. 214 (formerly 1172, 443)
# $Revision: 2.4 $
#
# Test functionality of PMC_Indom
#
# Copyright (c) 1995-2002,2005 Silicon Graphics, Inc.  All Rights Reserved.
#
# creator
owner=chatz

seq=`basename $0`
echo "QA output created by $seq"

. ./localconfig
if [ ! -f src-oss/pmc_indom ]
then
    echo "Not run: no src-oss/pmc_indom executable" >$seq.notrun
    echo "$seq: [not run] `cat $seq.notrun`"
    exit 0
fi

# get standard filters
. ./common.filter
. ./common.check

tmp=/tmp/$$
sudo=`pwd`/sudo
status=1	# failure is the default!
trap "rm -f $tmp.*; exit \$status" 0 1 2 3 15

# real QA test starts here

cd src-oss
./pmc_indom

# success, all done
status=0
exit
